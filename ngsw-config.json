{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html"],
        "versionedFiles": [
          "/**/*.bundle.css",
          "/**/*.bundle.js",
          "/**/*.chunk.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": ["https://cdn.contentful.com/spaces/**"],

      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "365d"
      }
    }
  ]
}
