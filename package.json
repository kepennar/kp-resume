{
  "name": "kp-resume",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod --base-href /kp-resume/",
    "build:client-server": "npm run build && npm run build:server",
    "build:prerender": "npm run build:client-server && npm run webpack:server && npm run generate:prerender",
    "build:server": "ng run kp-resume:server:production",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer kp-resume/stats.json",
    "deploy": "npm run build:prerender && gh-pages -d kp-resume",
    "e2e": "ng e2e",
    "generate:prerender": "cd kp-resume && node prerender",
    "lint": "ng lint",
    "ng": "ng",
    "prettyall": "prettier --single-quote --write \"src/**/*.{ts, scss}\"",
    "start": "ng serve --prod --source-map",
    "test": "ng test",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/platform-server": "^6.0.7",
    "@angular/router": "6.0.7",
    "@angular/service-worker": "6.0.7",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "animejs": "^2.2.0",
    "core-js": "^2.5.7",
    "rxjs": "6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.5.0",
    "codelyzer": "~4.3.0",
    "gh-pages": "^1.2.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "normalize.css": "^8.0.0",
    "prettier": "^1.13.7",
    "protractor": "^5.3.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^6.0.5",
    "tslint": "~5.9.1",
    "typescript": "~2.7.0",
    "webpack": "^4.14.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8"
  }
}
